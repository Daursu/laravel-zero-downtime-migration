{
    "name": "l-alexandrov/laravel-zero-downtime-migration",
    "description": "Zero downtime migrations with Laravel and percona toolkit",
    "require": {
        "php": "^7.0|^8.0|^8.1",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "symfony/console": "^3.3|^4.0|^5.0|^6.0",
        "symfony/process": "^3.3|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2|^8.4|^9.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "LAlexandrov\\ZeroDowntimeMigration\\": "src"
        }
    },
    "conflict": {
        "daursu/laravel-zero-downtime-migration": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/phpcs --standard=PSR2 --colors src/ tests/"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lyuboslav Alexandrov",
            "email": "lyubo@anvilo.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LAlexandrov\\ZeroDowntimeMigration\\ServiceProvider"
            ]
        }
    }
}